<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoginController">
	<children>
		<Label layoutX="179.0" layoutY="176.0" text="Usuário">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<TextField fx:id="txtUsuario" layoutX="128.0" layoutY="206.0" promptText="Digite o usuário" />
		<Label layoutX="183.0" layoutY="250.0" text="Senha">
			<font>
				<Font size="14.0" />
			</font>
		</Label>
		<TextField fx:id="txtSenha" layoutX="128.0" layoutY="278.0" promptText="Digite a senha" />
		<Button fx:id="btnEntrar" layoutX="145.0" layoutY="364.0" mnemonicParsing="false" onAction="#onBtnEntrarAction" prefHeight="26.0" prefWidth="114.0" text="Entrar" />
		<Button fx:id="btnCadastrar" layoutX="145.0" layoutY="407.0" mnemonicParsing="false" onAction="#onBtnCadastrarAction" text="Sou novo por aqui" />
		<Label layoutX="200.0" layoutY="304.0" text="Esqueci a senha">
			<font>
				<Font size="11.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
